{
  "name": "bpcli",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "build": "rimraf ./dist && tsc",
    "lint": "eslint .",
    "prettier:ts": "prettier 'src/**/*.{ts,tsx}' --write",
    "prepare": "husky"
  },
  "bin": {
    "bpcli": "./dist/index.js"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "eslint --fix",
      "pnpm run prettier:ts"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@eslint/js": "^9.12.0",
    "eslint": "^9.12.0",
    "eslint-config-prettier": "^9.1.0",
    "globals": "^15.11.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.9.0"
  }
}
